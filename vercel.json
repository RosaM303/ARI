{
    "version": 2,
    "builds": [
      {
        "src": "ari-backend/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/ari-backend/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  